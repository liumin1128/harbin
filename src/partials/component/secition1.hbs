   <div class="mb-5 mt-5">
      <h3 class="mb-4">前端控制的表单校验</h3>

      <div class="form max-width-600">

         <form class="row component-form-id-3" novalidate>
            <fieldset>
               <legend>
                  <p class="h3 mb-4"></p>
               </legend>


               <div class="mb-4">
                  <label for="disabledTextInput" class="form-label ">校验成功的输入框</label>
                  <input name="input1" type="text" id="disabledTextInput" class="form-control" placeholder="企业基本信息"
                     required>
                  <div class="valid-feedback">
                     Success message
                  </div>
                  <div class="invalid-feedback">
                     Error message
                  </div>
               </div>

               <div class="mb-4">
                  <label for="disabledTextInput" class="form-label">多行文本输入框</label>
                  <textarea placeholder="这就是占位" class="form-control" id="exampleFormControlTextarea1" rows="3"
                     required></textarea>
                  <div class="valid-feedback">
                     Looks good!
                  </div>
                  <div class="invalid-feedback">
                     Error message
                  </div>
               </div>

               <div class="d-flex justify-content-evenly">
                  <button type="submit" class="btn btn-primary">下一步</button>
               </div>

            </fieldset>


         </form>


         <script>


            // Fetch all the forms we want to apply custom Bootstrap validation styles to
            var forms = document.querySelectorAll('.component-form-id-3')

            // Loop over them and prevent submission
            Array.prototype.slice.call(forms)
               .forEach(function (form) {
                  form.addEventListener('submit', function (event) {
                     if (!form.checkValidity()) {
                        event.preventDefault()
                        event.stopPropagation()
                     }

                     form.classList.add('was-validated')

                     event.preventDefault()
                     event.stopPropagation()
                  }, false)
               })
         </script>


      </div>
